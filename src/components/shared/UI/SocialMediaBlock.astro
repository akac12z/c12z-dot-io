---
import Github from "@/common/ui/icons/social/Github.astro";
import LinkedIn from "@/common/ui/icons/social/LinkedIn.astro";
import Substack from "@/common/ui/icons/social/Substack.astro";
import X from "@/common/ui/icons/social/X.astro";

interface Props {
  width?: number;
  height?: number;
}

const { width = 20, height = 20 }: Props = Astro.props;
---

