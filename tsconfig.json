{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@sharedComponentsInPages/*": [
        "./src/components/pages/shared/*"
      ],
      "@analytics/*": [
        "./src/components/shared/analytics/*"
      ],
      "@home/*": [
        "./src/components/static/home/*"
      ],
      "@data/*": [
        "./src/data/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@essay/*": [
        "./src/components/essay/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@imageAssets/*": [
        "./src/assets/images/*"
      ],
      "@test/*": [
        "./src/components/test/*"
      ],
      
      
      "@commonIcons/*": [
        "./src/components/common/icons/*"
      ],
      "@basicPageComponents/*": [
        "./src/components/pages/content/basics/*"
      ],
      "@errorPageComponents/*": [
        "./src/components/pages/content/404/*"
      ],
      "@bookPageComponents/*": [
        "./src/components/pages/content/books/*"
      ],
      "@bookImages/*": [
        "./src/components/assets/images/books/*"
      ],
      "@biasPageComponents/*": [
        "./src/components/pages/content/behavior/sub_pages/bias/*"
      ],
      "@essayPageComponents/*": [
        "./src/components/pages/content/essay/*"
      ],
      "@projectPageComponents/*": [
        "./src/components/pages/content/projects/*"
      ],
      "@commonComponents/*": [
        "./src/components/common/*"
      ],
      "@commonPageComponents/*": [
        "./src/components/pages/common/*"
      ],
      "@btnCommonPageComponents/*": [
        "./src/components/pages/common/bottons/*"
      ],
      "@in_headComponents/*": [
        "./src/components/pages/in_head/*"
      ],
      
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}